{
  "name": "puppeteer-renderer-middleware",
  "author": {
    "name": "Yeongjin Lee",
    "email": "yeongjinnn@gmail.com",
    "url": "https://github.com/zenato"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenato/puppeteer-renderer"
  },
  "homepage": "https://github.com/zenato/puppeteer-renderer/tree/main/packages/middleware",
  "version": "1.0.0",
  "description": "An Express middleware for SSR using puppeteer-renderer",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsup src/index.ts --watch --onSuccess \"node dist/index.js\"",
    "build": "tsup src/index.ts",
    "clean": "rimraf {dist,coverage}",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.197",
    "express": "^4.18.1",
    "lodash": ">=4.17.19",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  },
  "keywords": [
    "puppeteer",
    "renderer",
    "express",
    "chrome",
    "headless"
  ],
  "files": [
    "dist"
  ]
}